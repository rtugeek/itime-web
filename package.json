    "lunar-typescript": "^1.7.5",
    "md5": "^2.3.0",
    "nanoid": "^5.0.7",
    "pinia": "^2.1.7",
    "rrule": "^2.8.1",
    "semver": "^7.6.3",
    "sortablejs": "^1.15.2",
    "vue": "^3.4.21",
    "vue-i18n": "10",
    "vue-router": "^4.3.0",
    "vue3-colorpicker": "^2.3.0",
    "vue3-emoji-picker": "^1.1.8"
