{
  "name": "itime-web",
  "version": "0.8.4",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "build:android": "vite build --mode android && npm run mobileStyle",
    "build:ios": "vite build --mode ios && npm run mobileStyle",
    "preview": "vite preview",
    "preview:ios": "vite preview --mode ios",
    "preview:android": "vite preview --mode android",
    "test:unit": "vitest",
    "type-check": "vue-tsc --build --force",
    "release": "widget release -t ftp",
    "update:widgetjs": "widget dependencies -t remote",
    "local": "widget dependencies -t local",
    "mobileStyle": "esno script/mobileRootStyle.ts",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
